<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>RentAWheel.</title>
    <link
      href="https://fonts.googleapis.com/css?family=Nunito:400,300"
      rel="stylesheet"
      type="text/css"
    />
    <link rel="stylesheet" href="/css/add-vehicles.css" />
    <link rel="stylesheet" href="/css/style.css" />
  </head>
  <body>
    <h1><a href="/adminpage">RentA<span id="wheel">Wheel.</span></a></h1>
    <div class="row">
      <div class="col-md-12">
        <form id="add-vehicles-form">
          <h2>Add Vehicle Details</h2>

          <fieldset>

            <legend><span class="number">#</span>
              the vehicles creadted will be assigned to your account</legend>

            <input placeholder="model" type="text" id="model" name="model" />

            <input placeholder="type" type="text" id="type" name="type" />

            <input
              placeholder="seat capacity"
              type="text"
              id="seatcapacity"
              name="seatcapacity"
            />

            <input
              placeholder="boot capacity"
              type="text"
              id="bootcapacity"
              name="bootcapacity"
            />

            <input
              placeholder="kilometers driven"
              type="text"
              id="kilometersdriven"
              name="kilometersdriven"
            />

            <input
              placeholder="price per day"
              type="text"
              id="rate"
              name="rate"
            />

            <input
              placeholder="available units"
              type="text"
              id="availablestock"
              name="availablestock"
            />

            <input
              placeholder="image url"
              type="text"
              id="image"
              name="image"
            />
          </fieldset>

          <button type="submit">Add.</button>

        </form>
      </div>
    </div>

  </body>
  <script src="https://unpkg.com/axios@0.25.0/dist/axios.min.js"></script>
  <script src="/js/addVehicle.js"></script>
</html>